{
    "openapi": "3.0.0",
    "info": {
        "title": "Cheese API",
        "description": "An API to return details of world-famous cheeses.",
        "version": "0.1.9"
    },
    "servers": [{
            "url": "http://cheese.com/",
            "description": "The legendary internet cheese server"
        }, {
            "url": "http://cheese-staging.ddns.net",
            "description": "A staging environment dedicated to testing new API versions."
        }
    ],
    "paths": {
        "/cheddar": {
            "get": {
                "summary": "Returns an individual cheese profile.",
                "description": "Optional extended description in CommonMark or HTML.",
                "responses": {
                    "200": {
                        "description": "Returns the profile of a famous cheese",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
